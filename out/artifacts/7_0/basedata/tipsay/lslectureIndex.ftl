			$(function(){
				var $walkthroughListHeight = $(".walkthroughList").height();
				$(".js-switch-table").height(770 - $walkthroughListHeight);
			})
			
			function changeClass(clazz){
				var url = "${request.contextPath}/basedata/lslecture/index/page2?clazz="+clazz;
				$("#model-div-86312").load(url);
			}
			
			$(function(){
			
				$(".center-mytable").find(".item").on('click',function(){
					$(".center-mytable").find(".item").removeClass("active");
					$(this).removeClass("choose");
					$(this).removeClass("appoint");
					$(this).removeClass("conflict");
					$(this).addClass("active");
					
					var teacherNames=$(this).attr("data-teacherName");
					if(teacherNames && teacherNames!=""){
						$(".right-up-teacher").html(teacherNames);
						$(".right-up-mytable1").hide();
						$(".right-up-mytable").show();
						
					}else{
						$(".right-up-teacher").html("");
						$(".right-up-mytable").hide();
						$(".right-up-mytable1").show();
					}
				})
				$(".center-mytable").find(".item").on('mousemove',function(){
					if($(".center-mytable").find(".active").length<=0){
						$(".right-down-teacher").html("");
						$(".right-down-mytable").hide();
						$(".right-down-mytable1").show();
						return;
					}
					if($(this).hasClass("active")){
						$(".right-down-teacher").html("");
						$(".right-down-mytable").hide();
						$(".right-down-mytable1").show();
					}else{
						var teacherNames=$(this).attr("data-teacherName");
						if(teacherNames && teacherNames!=""){
							$(".right-down-teacher").html(teacherNames);
							$(".right-down-mytable1").hide();
							$(".right-down-mytable").show();
						}else{
							
							$(".right-down-teacher").html("");
							$(".right-down-mytable1").hide();
							$(".right-down-mytable").show();
						}
					}
					
				})
			})
		</script>
	