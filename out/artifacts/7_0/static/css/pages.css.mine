.steps li:before{position:relative;top:18px;display:block;content:'';height:3px;background-color:#bcc2c4;}
.steps li.first:before{left:50%;width:50%;}
.steps li.last:before{width:50%;}
.step{display:inline-block;width:32px;height:32px;border-radius:50%;background-color:#bcc2c4;}
.steps p{color:#999;}
.steps li.active:before,
.steps li.active .step,
.steps li.completed:before,
.steps li.completed .step{background-color:#2c9ae8;}
.steps li.active a{color:#2c9ae8;}

.stepsContainer{margin-left: -110px;text-align: center;}
.steps-default{display:inline-block;padding-bottom:20px;}
